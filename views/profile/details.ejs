<% users.forEach(user => { %>

<h1><%= user.username %></h1>
<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>About Me</th>
        <th>Age</th>
        <th>Posts</th>
    <% if(currentUser && user.username === currentUser.username) { %>
        <th><a href="/profile/edit/<%= user.username %>">Edit</a></th>
        <th><a href="/profile/changepass/<%= user.username %>">change password</a></th>
    <% } %>
    </tr>

    <tr>
        <td><%= user.firstName %></td>
        <td><%= user.lastName %></td>
        <td><%= user.aboutMe %></td>
        <td><%= user.age %></td>
        <td><%= user.post %></td>
    </tr>

</table>

<% }) %>